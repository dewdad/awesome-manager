{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "打开Chrome，端口9229",
      "url": "http://localhost:9229",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "通过node调试fp程序，端口随机",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/lib/util/bin/fp.js"
    },
    {
      "name": "通过Yarn调试fp程序，端口9229",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "debug"
      ],
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "打开Electron开发服务器，端口8080",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "dev"
      ],
      "port": 8080
    },
    {
      "type": "node",
      "name": "使用Jest测试",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/jest/bin/jest",
      "args": [
        "--runInBand"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "electron",
      "request": "launch",
      "name": "启动Electron调试安装压缩包",
      "appDir": "/dist_electron/bundled",
      "sourceMaps": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "启动storybook，端口6006",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "storybook:serve",
      ],
      "port": 6006
    },
  ]
}