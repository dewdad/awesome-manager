{
  "name": "awesome-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn electron:serve",
    "dist": "yarn electron:build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:storybook": "vue-cli-service build:storybook -c config/storybook",
    "install:plugins": "vue add i18n && vue add prisma",
    "rake:install": "yarn add -D file:lib/vue-cli-plugins/vue-cli-plugin-vuex-db-module-generator",
    "rake:invoke": "vue invoke vuex-db-module-generator",
    "serve:storybook": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "@types/webpack": "^4.4.17",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "register-service-worker": "^1.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^3.0.0",
    "@storybook/addon-actions": "4.0.0-alpha.20",
    "@storybook/addon-links": "4.0.0-alpha.20",
    "@types/jest": "^23.1.4",
    "@types/nedb": "^1.8.6",
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-pwa": "^3.0.1",
    "@vue/cli-plugin-typescript": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.5",
    "@vue/eslint-config-typescript": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "electron": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-electron-builder": "^1.0.0-rc.7",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-cli-plugin-storybook": "^0.4.8",
    "vue-cli-plugin-vuetify": "^0.4.5",
    "vue-cli-plugin-vuex-db-module-generator": "file:lib/vue-cli-plugins/vue-cli-plugin-vuex-db-module-generator",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.5",
    "vuex-class": "^0.3.1",
    "vuex-pathify": "^1.1.3"
  },
  "main": "background.js"
}
