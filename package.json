{
    "name": "awesome-manager",
    "version": "1.0.6",
    "private": true,
    "description": "awesome management",
    "author": "linuxing3<linuxing3@qq.com>",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "clean": "rm -rf electron_dist",
        "db:add": "yarn add -D vue-cli-plugin-vuex-nedb-module-generator",
        "db:invoke": "vue invoke vuex-nedb-module-generator",
        "dev": "yarn electron:serve",
        "dist": "yarn electron:build",
        "electron:build": "vue-cli-service electron:build",
        "electron:build:legacy": "vue-cli-service electron:build --legacy",
        "electron:build:skip": "vue-cli-service electron:build --skipBundle",
        "electron:serve": "vue-cli-service electron:serve",
        "new": "hygen new",
        "new:all": "hygen new model && hygen new router && hygen new component-table && hygen new component-form",
        "play": "run-p play:watch play:debug",
        "play:debug": "node --nolazy --inspect-brk=9229 ./lib/util/bin/fp.js",
        "play:watch": "tsc -p ./lib/util --watch",
        "plugins:install": "vue add vuetify && vue add pug && vue add storybook && vue add i18n && vue add prisma",
        "postinstall": "electron-builder install-app-deps",
        "prisma:add": "yarn add vue-cli-plugin-prisma",
        "prisma:demo": "ts-node -p src/api/prisma src/api/prisma/demo.ts",
        "prisma:deploy": "cd lib/prisma && prisma deploy -e ../../.env",
        "prisma:generate": "cd lib/prisma && prisma generate",
        "prisma:now": "cd lib/prisma && sh config-now.sh && now",
        "release:major": "git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version major && git push origin && git push origin --tags",
        "release:minor": "git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version minor && git push origin && git push origin --tags",
        "release:patch": "git add CHANGELOG.md && git commit -m 'chore: update changelog' && npm version patch && git push origin && git push origin --tags",
        "storybook:build": "vue-cli-service build:storybook -c config/storybook",
        "storybook:serve": "vue-cli-service serve:storybook -p 6006 -c config/storybook",
        "inspect": "vue inspect",
        "test:e2e": "vue-cli-service test:e2e",
        "test:unit": "vue-cli-service test:unit",
        "test:unit:watch": "vue-cli-service test:unit --watch"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "bcryptjs": "^2.4.3",
        "csv": "^4.0.0",
        "jszip": "^3.1.5",
        "lodash-id": "^0.14.0",
        "lowdb": "^1.0.0",
        "nedb": "^1.8.0",
        "nedb-promise": "^2.0.1",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "papaparse": "^4.6.1",
        "register-service-worker": "^1.0.0",
        "rxjs": "^6.3.3",
        "vue": "^2.5.17",
        "vue-class-component": "^6.0.0",
        "vue-i18n": "^8.0.0",
        "vue-perfect-scrollbar": "^0.1.0",
        "vue-property-decorator": "^7.0.0",
        "vue-router": "^3.0.1",
        "vue-rx": "^6.0.1",
        "vue-tsx-support": "^2.1.1",
        "vuetify": "^1.3.0",
        "vuex": "^3.0.1",
        "@vuex-orm/core": "^0.29.0",
        "vuex-class": "^0.3.1",
        "vuex-pathify": "^1.1.3",
        "vuex-orm-lowdb": "^0.2.7",
        "xlsx": "^0.14.1"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^3.0.0",
        "@fortawesome/fontawesome-free": "^5.4.2",
        "@kazupon/vue-i18n-loader": "^0.3.0",
        "@storybook/addon-actions": "4.0.0-alpha.20",
        "@storybook/addon-links": "4.0.0-alpha.20",
        "@types/jest": "^23.1.4",
        "@types/lowdb": "^1.0.6",
        "@types/nedb": "^1.8.6",
        "@types/webpack": "^4.4.17",
        "@vue/cli-plugin-babel": "^3.0.1",
        "@vue/cli-plugin-e2e-cypress": "^3.0.1",
        "@vue/cli-plugin-eslint": "^3.0.1",
        "@vue/cli-plugin-pwa": "^3.0.1",
        "@vue/cli-plugin-typescript": "^3.0.1",
        "@vue/cli-plugin-unit-jest": "^3.0.1",
        "@vue/cli-service": "^3.0.1",
        "@vue/eslint-config-prettier": "^3.0.5",
        "@vue/eslint-config-typescript": "^3.0.5",
        "@vue/test-utils": "^1.0.0-beta.20",
        "babel-core": "7.0.0-bridge.0",
        "babel-loader": "^8.0.4",
        "echarts": "^4.2.0-rc.2",
        "electron": "^2.0.0",
        "graphql": "^14.0.0",
        "hygen": "1.6.x",
        "material-design-icons-iconfont": "^4.0.3",
        "node-sass": "^4.10.0",
        "normalizr": "^3.3.0",
        "npm-run-all": "^4.1.5",
        "prisma-binding": "^2.2.14",
        "prisma-client-lib": "^1.25.1",
        "roboto-fontface": "^0.10.0",
        "sass-loader": "^7.1.0",
        "script-loader": "^0.7.2",
        "shelljs": "^0.8.3",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "ts-jest": "^23.0.0",
        "ts-node": "7.0.1",
        "typescript": "^3.0.0",
        "vue-cli-plugin-electron-builder": "^1.0.0-rc.7",
        "vue-cli-plugin-i18n": "^0.5.1",
        "vue-cli-plugin-pug": "^1.0.7",
        "vue-cli-plugin-rx": "^0.1.5",
        "vue-cli-plugin-storybook": "^0.4.8",
        "vue-cli-plugin-vuetify": "^0.4.5",
        "vue-jsx-hot-loader": "^1.4.0",
        "vue-quill-editor": "^3.0.6",
        "vue-strict-prop": "^0.3.0",
        "vue-template-compiler": "^2.5.17",
        "vuetify-loader": "^1.0.5",
        "vuetify-tsx": "^0.16.2"
    },
    "license": "MIT",
    "main": "background.js"
}
